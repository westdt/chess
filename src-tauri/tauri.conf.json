{
	"build": {
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "chess",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			},
			"fs": {
				"scope": [
					"$RESOURCE/*"
				],
				"all": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "com.westdt.chess",
			"icon": [
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"resources": []
		},
		"security": {
			"csp": null
		}
	}
}